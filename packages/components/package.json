{
  "name": "@powerfulyang/components",
  "version": "0.7.5",
  "description": "react-components",
  "author": "powerfulyang",
  "license": "MIT",
  "repository": "https://github.com/powerfulyang/tools.git",
  "files": [
    "dist"
  ],
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "style": "dist/es/index.css",
  "sideEffects": false,
  "scripts": {
    "prettier": "prettier --write \"src/**/*.{js,jsx,tsx,ts,json,md}\"",
    "fix:style": "stylelint --fix \"src/**/*.scss\" --syntax scss",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "build": "rimraf dist && cross-env NODE_ENV=production rollup -c",
    "build:dev": "rimraf dist && cross-env NODE_ENV=development rollup -c",
    "watch:build": "rimraf dist && cross-env NODE_ENV=development rollup -c --watch"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "@mojs/core": "^0.288.2",
    "@powerfulyang/hooks": "^0.2.5",
    "@powerfulyang/utils": "^0.7.5",
    "react-animate-height": "^2.0.23",
    "react-markdown": "^5.0.2",
    "react-syntax-highlighter": "^15.3.0",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@powerfulyang/lint": "^0.2.5",
    "@powerfulyang/node-utils": "^0.2.5",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-url": "^5.0.1",
    "@svgr/rollup": "^5.5.0",
    "@types/classnames": "^2.2.11",
    "@types/lodash-es": "^4.17.3",
    "@types/react-dom": "^16.9.9",
    "@types/react-syntax-highlighter": "^13.5.0",
    "autoprefixer": "^9.8.6",
    "lodash-es": "^4.17.15",
    "node-sass": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^2.33.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "tailwindcss": "^1.9.6"
  }
}
